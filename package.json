{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --logLevel info",
    "build": "run-p type-check \"build-only {@}\" --",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:browser": "vitest --browser",
    "test:unit": "vitest run tests/unit",
    "test:components": "vitest run tests/components",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/browser",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "staging": "vite build --mode staging",
    "production": "vite build --mode production",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@achrinza/node-ipc": "^10.1.1",
    "@primeuix/themes": "^1.1.2",
    "@primevue/auto-import-resolver": "^4.3.5",
    "@primevue/forms": "^4.3.5",
    "@tailwindcss/vite": "^4.1.10",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "material-design-icons-iconfont": "^6.7.0",
    "nanoid": "^5.1.5",
    "node-forge": "^1.3.1",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "tailwindcss-primeui": "^0.6.1",
    "unplugin-vue-components": "^28.7.0",
    "vue": "^3.5.13",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.1",
    "@types/node-forge": "^1.3.11",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/browser": "^3.2.4",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/eslint-config-prettier": "^10.1.5",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "happy-dom": "^18.0.1",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.53.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-singlefile": "^2.2.0",
    "vitest": "^3.2.4",
    "vitest-browser-vue": "^1.0.0",
    "vue-tsc": "^2.2.10"
  }
}
